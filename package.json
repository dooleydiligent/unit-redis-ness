{
  "name": "unit-redis-ness",
  "version": "1.0.3",
  "description": "A typescript implementation of redis to facilitate unit testing",
  "main": "dist/server/resp-server.js",
  "messages": [
    "adapted from https://github.com/tonivade/claudb.git"
  ],
  "bugs": {
    "url": "https://github.com/dooleydiligent/unit-redis-ness/issues"
  },
  "files": [
    "dist",
    "images",
    "test-unit-redis-ness"
  ],
  "homepage": "https://dooleydiligent.github.io/unit-redis-ness/",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run doc && npm run makeindex && npm test && tsc -p .",
    "clean": "rm -rf ${npm_package_name}-${npm_package_version}.tgz dist doc coverage .nyc_output test-unit-redis-ness/node_modules docs/*/*",
    "doc": "typedoc --mode file --out docs/doc --theme minimal --target ES6 --ignoreCompilerErrors --tsconfig ./tsconfig.json --hideGenerator --exclude **/*.test.ts src node_modules/@types/node && cp -R images ./docs/doc/images",
    "makeindex": "echo \"<HTML><HEAD><TITLE>${npm_package_name}-v${npm_package_version}</TITLE></HEAD><BODY>${npm_package_description}<br><a href='./coverage/index.html'>Code Coverage</a><br><a href='./doc/index.html'>Documentation</a><br></BODY></HTML>\" > docs/index.md",
    "lint": "tslint --project . --format prose --force",
    "start": "node -e 'const s = require(\"./dist/server/resp-server\");const c=new s.RespServer();c.start();'",
    "test": "nyc --reporter=cobertura --reporter=html mocha --recursive --exit test",
    "prepack": "npm run clean && npm run build && npm run doc && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/laneholcombe/unit-redis-ness.git"
  },
  "keywords": [
    "redist",
    "typescript"
  ],
  "author": "lane holcombe",
  "license": "MIT",
  "dependencies": {
    "generate-safe-id": "^1.0.3",
    "redis-sorted-set": "^2.0.0",
    "resp": "^0.1.1"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/chai": "^4.2.7",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.4",
    "@types/sinon": "^7.5.1",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "deep-equal": "^2.0.1",
    "mocha": "^6.2.2",
    "net": "^1.0.2",
    "nyc": "^15.0.0",
    "redis-parser": "^3.0.0",
    "sinon": "^8.0.2",
    "testdouble": "^3.12.4",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "spec"
    ],
    "sourceMap": true,
    "instrument": true,
    "report-dir": "docs/coverage"
  }
}
