{
  "name": "unit-redis",
  "version": "1.0.0",
  "description": "A typescript implementation of redis",
  "main": "dist/redis.js",
  "messages": [
    "adapted from https://github.com/meteor/miniredis"
  ],
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist doc coverage checkstyle-result.xml .nyc_output",
    "doc": "typedoc --mode file --out doc --theme minimal --target ES6 --ignoreCompilerErrors --tsconfig ./tsconfig.json --hideGenerator --exclude **/*.test.ts src node_modules/@types/node",
    "lint": "tslint --project . --format prose --force",
    "start": "node -e 'const s = require(\"./dist/server/resp-server\");const c=new s.RespServer();c.start();'",
    "test": "nyc --reporter=cobertura --reporter=html mocha --recursive --exit test"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "redist",
    "typescript"
  ],
  "author": "lane holcombe",
  "license": "ISC",
  "dependencies": {
    "generate-safe-id": "^1.0.3",
    "redis-sorted-set": "^2.0.0",
    "resp": "^0.1.1"
  },
  "devDependencies": {
    "@types/assert": "^1.4.3",
    "@types/chai": "^4.2.7",
    "@types/deep-equal": "^1.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.2",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "deep-equal": "^2.0.1",
    "mocha": "^6.2.2",
    "net": "^1.0.2",
    "nyc": "^15.0.0",
    "redis-parser": "^3.0.0",
    "sinon": "^8.0.2",
    "testdouble": "^3.12.4",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "spec"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
